{% extends 'layout.njk' %}

{% block title %}YESM 2020 | Sponsors{% endblock %}
{% block desc %}Sponsors of YESM. Young Economist Students' Meet 2019 (YESM 2019). Venue: Jadavpur University. Department of Economics. Designer: Vedant Mozumdar.{% endblock %}

{% block metahead %}
    <link rel="stylesheet" href="css/sponsor.css">
    <script src="js/sponsor.js"></script>
{% endblock %}

{% block content %}
<body id="sponsor">
    {% import 'macros/header.njk' as nav %}
    {{ nav.gen('s') }}
    <main id="main">
        <section>
            <h1>Sponsors</h1>
            <div>
                <div class="img-wrap" style="width:25vw;height:120vw;max-width:600px;max-height:250px;">
                    <img class="lazy" data-src="WBPDCL Logo_Blue Colour.jpg" src="data:image/jpeg;base64," alt="Hatari">
                </div>
                <p>Title Sponsor</p>
            </div>
            <div>
                <div class="img-wrap" style="width:80vw;height:45vw;max-width:444px;max-height:250px;">
                    <img class="lazy" data-src="GAFA-Logo-NEw.png" src="data:image/jpeg;base64," alt="Gafa">
                </div>
                <p>Knowledge Partner</p>
            </div>
            <div>
                <div class="img-wrap" style="width:300px;height:250px;">
                    <img class="lazy" data-src="catalyst.jpeg" src="data:image/jpeg;base64," alt="Catalyst Learning Services">
                </div>
                <p>Career Partner</p>
            </div>
            <div>
                <div class="img-wrap" style="width:345px;height:213px;">
                    <img class="lazy" data-src="edusure.png" src="data:image/jpeg;base64," alt="Edusure">
                </div>
                <p>Education Partner</p>
            </div>
            <div>
                <div class="img-wrap" style="width:300px;height:250px;">
                    <h1 style="position: absolute; top:50%; transform: translateY(-50%);">Competitive Success Academy</h1>
                </div>
                <p>Associate Sponsor</p>
            </div>
            <div>
                <div class="img-wrap" style="width:299px;height:250px;">
                    <img class="lazy" data-src="grissini.jpg" src="data:image/jpeg;base64," alt="Grissini">
                </div>
                <p>Associate Sponsor</p>
            </div>
            <div>
                <div class="img-wrap" style="width:300px;height:300px;">
                    <img class="lazy" data-src="KMDB_logo.png" src="data:image/jpeg;base64," alt="KMDB">
                </div>
                <p>Gift Sponsor</p>
            </div>
        </section>
    </main>
</body>
{% endblock %}
